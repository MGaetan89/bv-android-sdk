<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".conversations.kitchensink.ConversationsFragment">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Review"
        android:id="@+id/reviewBtn"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/cmtStoryBtn"
        android:layout_alignEnd="@+id/cmtStoryBtn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stories"
        android:id="@+id/storiesBtn"
        android:layout_below="@+id/textView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/categoriesBtn"
        android:layout_alignStart="@+id/categoriesBtn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Answers"
        android:id="@+id/answerBtn"
        android:layout_below="@+id/reviewBtn"
        android:layout_alignRight="@+id/reviewBtn"
        android:layout_alignEnd="@+id/reviewBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="Categories"
        android:id="@+id/categoriesBtn"
        android:layout_below="@+id/storiesBtn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comments"
        android:id="@+id/commentsBtn"
        android:layout_below="@+id/answerBtn"
        android:layout_alignRight="@+id/answerBtn"
        android:layout_alignEnd="@+id/answerBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profiles"
        android:id="@+id/profileBtn"
        android:layout_alignBottom="@+id/commentsBtn"
        android:layout_alignLeft="@+id/categoriesBtn"
        android:layout_alignStart="@+id/categoriesBtn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Products"
        android:id="@+id/productBtn"
        android:layout_below="@+id/commentsBtn"
        android:layout_alignRight="@+id/commentsBtn"
        android:layout_alignEnd="@+id/commentsBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Question"
        android:id="@+id/questionBtn"
        android:layout_alignTop="@+id/productBtn"
        android:layout_alignLeft="@+id/profileBtn"
        android:layout_alignStart="@+id/profileBtn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="Comment Story"
        android:id="@+id/cmtStoryBtn"
        android:layout_below="@+id/productBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Statistics"
        android:id="@+id/statsBtn"
        android:layout_alignTop="@+id/cmtStoryBtn"
        android:layout_alignLeft="@+id/questionBtn"
        android:layout_alignStart="@+id/questionBtn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Content Display"
        android:id="@+id/textView"
        android:layout_marginTop="20dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Content Submission"
        android:id="@+id/textView2"
        android:layout_below="@+id/cmtStoryBtn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Review"
        android:id="@+id/reviewSubBtn"
        android:layout_below="@+id/textView2"
        android:layout_alignRight="@+id/cmtStoryBtn"
        android:layout_alignEnd="@+id/cmtStoryBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Question"
        android:id="@+id/questionSubBtn"
        android:layout_below="@+id/reviewSubBtn"
        android:layout_alignRight="@+id/reviewSubBtn"
        android:layout_alignEnd="@+id/reviewSubBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Answer"
        android:id="@+id/answerSubBtn"
        android:layout_below="@+id/questionSubBtn"
        android:layout_alignRight="@+id/cmtStoryBtn"
        android:layout_alignEnd="@+id/cmtStoryBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stories"
        android:id="@+id/storiesSubBtn"
        android:layout_below="@+id/answerSubBtn"
        android:layout_alignRight="@+id/cmtStoryBtn"
        android:layout_alignEnd="@+id/cmtStoryBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comments"
        android:id="@+id/cmtSubBtn"
        android:layout_below="@+id/textView2"
        android:layout_alignLeft="@+id/statsBtn"
        android:layout_alignStart="@+id/statsBtn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Photos"
        android:id="@+id/photoSubBtn"
        android:layout_below="@+id/cmtSubBtn"
        android:layout_alignLeft="@+id/cmtSubBtn"
        android:layout_alignStart="@+id/cmtSubBtn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Videos"
        android:id="@+id/videoSubBtn"
        android:layout_below="@+id/photoSubBtn"
        android:layout_alignLeft="@+id/photoSubBtn"
        android:layout_alignStart="@+id/photoSubBtn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="      Feedback      "
        android:id="@+id/feedbackSubBtn"
        android:layout_below="@+id/videoSubBtn"
        android:layout_alignLeft="@+id/videoSubBtn"
        android:layout_alignStart="@+id/videoSubBtn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
